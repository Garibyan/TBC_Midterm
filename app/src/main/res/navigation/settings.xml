<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings"
    app:startDestination="@id/settingsFragment">

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.garibyan.armen.tbc_midterm.view.tabs.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_passwordResetFragment"
            app:destination="@id/passwordResetFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_welcomeFragment2"
            app:destination="@id/welcomeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/passwordResetFragment"
        android:name="com.garibyan.armen.tbc_midterm.view.tabs.PasswordResetFragment"
        android:label="PasswordResetFragment" />
    <fragment
        android:id="@+id/welcomeFragment2"
        android:name="com.garibyan.armen.tbc_midterm.view.auth.WelcomeFragment"
        android:label="WelcomeFragment" />
</navigation>